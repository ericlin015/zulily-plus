{
  "manifest_version": 2,

  "name": "Zulily Plus",
  "description": "Stuff",
  "version": "1.0",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": [
    "https://www.zulily.com/",
    "http://www.zulily.com/"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.zulily.com/*"],
      "js": ["background.js"]
    }
  ]
}